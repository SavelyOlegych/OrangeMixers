{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/vue3-essential",
    "@vue/standard",
    "@vue/typescript"
  ],
  "parserOptions": {
    "parser": "@typescript-eslint/parser"
  },
  "rules": {
    "indent": [
      "error",
      4
    ],
    "semi": 0,
    "vue/multi-word-component-names": "off",
    "space-before-function-paren": "off",
    "comma-dangle": "off"
  }
}